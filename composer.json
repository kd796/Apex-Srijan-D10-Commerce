    "name": "vmlyr-drupal/kit",
    "description": "Project template for VMLY&R Drupal 8 projects with composer.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Garrett Rathbone",
            "role": "Technology Specialist"
        }
